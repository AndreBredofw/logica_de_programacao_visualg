algoritmo "semnome"
var
      c: inteiro
      reserva: caractere
inicio
      para c de 1 ate 5 faca
                Escreva("[B", c ,"]")
      fimpara
      Escreval("")
      repita
           Escreva("RESERVAR A CADEIRA: ")
           leia(reserva)
           Escreval("Quer continuar? [s/n]")
      ate (reserva= "n")
fimalgoritmo